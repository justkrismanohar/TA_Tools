import java.io.File;
import java.io.IOException;

import org.apache.poi.ss.usermodel.Workbook;
import org.apache.poi.xssf.usermodel.XSSFWorkbook;

public class MasterSheet {
	
	private Workbook masterSheet;
	private
	public MasterSheet(String dirPath,String name){
		try {
			masterSheet = new XSSFWorkbook(dirPath+File.pathSeparator+name);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
	
	
}
